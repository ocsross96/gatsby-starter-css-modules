{
  "extends": "plugin:shopify/react",
  "env": {
      "browser": true,
      "node": true,
      "jest": true
  },
  "newline-after-import": false,
  "rules": {
    "babel/object-curly-spacing": 0,
    "babel/no-invalid-this": 0,
    "newline-per-chained-call": 0,
    "no-useless-escape": 0,
    "no-useless-return": 0,
    "no-negated-condition": 0,
    "comma-dangle": [2,"never"],
    "no-cond-assign": [2,"always"],
    "no-console": 0,
    "no-constant-condition": 2,
    "no-control-regex": 0,
    "no-debugger": 0,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 0,
    "no-empty": 2,
    "no-ex-assign": 0,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 0,
    "no-invalid-regexp": 0,
    "no-irregular-whitespace": 0,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 0,
    "no-regex-spaces": 0,
    "no-sparse-arrays": 0,
    "no-unreachable": 2,
    "use-isnan": 0,
    "valid-jsdoc": 0,
    "valid-typeof": 2,
    "no-unexpected-multiline": 2,
    "accessor-pairs": 0,
    "block-scoped-var": 2,
    "complexity": 0,
    "consistent-return": 0,
    "curly": [2,"all"],
    "default-case": 0,
    "dot-notation": 2,
    "dot-location": 0,
    "eqeqeq": 2,
    "guard-for-in": 0,
    "no-alert": 2,
    "no-caller": 0,
    "no-div-regex": 0,
    "no-else-return": 2,
    "no-empty-label": 0,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-extend-native": 0,
    "no-extra-bind": 0,
    "no-fallthrough": 0,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 0,
    "no-implied-eval": 0,
    "no-invalid-this": 0,
    "no-iterator": 0,
    "no-labels": 0,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 0,
    "no-octal": 0,
    "no-param-reassign": 2,
    "no-process-env": 0,
    "no-proto": 0,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 0,
    "no-self-compare": 2,
    "no-sequences": 0,
    "no-throw-literal": 2,
    "no-unused-expressions": 2,
    "no-useless-call": 0,
    "no-void": 0,
    "no-warning-comments": 0,
    "no-with": 1,
    "radix": 0,
    "vars-on-top": 1,
    "wrap-iife": 0,
    "yoda": 0,
    "strict": 0,
    "init-declarations": 0,
    "no-catch-shadow": 0,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow-restricted-names": 0,
    "no-shadow": 2,
    "no-undef-init": 0,
    "no-undef": 2,
    "no-undefined": 0,
    "no-unused-vars": 1,
    "no-use-before-define": 2,
    "callback-return": 0,
    "handle-callback-err": 0,
    "no-mixed-requires": 2,
    "no-new-require": 2,
    "no-path-concat": 1,
    "no-process-exit": 1,
    "no-restricted-modules": 0,
    "no-sync": 2,
    "array-bracket-spacing": 0,
    "brace-style": [2,"1tbs",{"allowSingleLine":true}],
    "camelcase": 0,
    "comma-spacing": 0,
    "comma-style": 0,
    "computed-property-spacing": [1,"never"],
    "consistent-this": 0,
    "eol-last": 1,
    "func-names": 0,
    "func-style": 0,
    "id-length": 0,
    "indent": 1,
    "key-spacing": [2,{"afterColon":true}],
    "lines-around-comment": 0,
    "linebreak-style": 0,
    "max-nested-callbacks": 0,
    "new-cap": 2,
    "new-parens": 1,
    "newline-after-var": 0,
    "no-array-constructor": 2,
    "no-continue": 0,
    "no-inline-comments": 0,
    "no-lonely-if": 0,
    "no-mixed-spaces-and-tabs": [2,"smart-tabs"],
    "no-multiple-empty-lines": [1,{"max":1}],
    "no-nested-ternary": 1,
    "no-new-object": 1,
    "no-spaced-func": 0,
    "no-ternary": 0,
    "no-trailing-spaces": [1,{"skipBlankLines":true}],
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 0,
    "object-curly-spacing": 0,
    "one-var": 0,
    "operator-assignment": 0,
    "operator-linebreak": 0,
    "padded-blocks": 0,
    "quote-props": 0,
    "quotes": 0,
    "id-match": 0,
    "semi-spacing": 0,
    "semi": 0,
    "sort-vars": 0,
    "space-after-keywords": 0,
    "space-before-blocks": 0,
    "space-in-parens": 0,
    "space-infix-ops": 0,
    "space-return-throw-case": 0,
    "space-unary-ops": 0,
    "spaced-comment": 0,
    "wrap-regex": 0,
    "arrow-parens": 1,
    "arrow-spacing": [1,{"before":true,"after":true}],
    "constructor-super": 2,
    "generator-star-spacing": 1,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-this-before-super": 1,
    "no-var": 2,
    "object-shorthand": 0,
    "prefer-const": 1,
    "prefer-spread": 0,
    "prefer-reflect": 0,
    "require-yield": 1,
    "max-depth": [1,2],
    "max-len": 0,
    "max-params": 0,
    "max-statements": 0,
    "no-bitwise": 0,
    "no-plusplus": 0,
    "class-methods-use-this": 0
  }
}